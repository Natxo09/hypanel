{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "HyPanel",
  "version": "0.1.0",
  "identifier": "com.natxo.hypanel",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "HyPanel",
        "width": 1200,
        "height": 800,
        "minWidth": 900,
        "minHeight": 600,
        "center": true,
        "resizable": true,
        "decorations": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "createUpdaterArtifacts": true
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEM0Q0IyQzY5Qzk2QTNCRDEKUldUUk8yckphU3pMeFA3ZURkOXJWM2hPb0N4cE5DRm9HMFp6NmV6TFd0TjV1d094WUhjaExVTmUK",
      "endpoints": [
        "https://github.com/Natxo09/hypanel/releases/latest/download/latest.json"
      ]
    }
  }
}
